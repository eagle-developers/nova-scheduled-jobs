{
    "name": "eagle-developers/nova-scheduled-tasks",
    "description": "A Laravel Nova card to view your scheduled tasks.",
    "keywords": [
        "laravel",
        "nova"
    ],
    "homepage": "https://github.com/eagle-developers/nova-scheduled-tasks",
    "license": "MIT",
    "authors": [
        {
            "name": "Eagle Metal Products Web Developers",
            "email": "webdev@eaglemetal.com",
            "homepage": "https://github.com/eagle-developers",
            "role": "Fork Developers"
        },
        {
            "name": "Larry Laski",
            "email": "larry.laski@gmail.com",
            "homepage": "https://larrylaski.com",
            "role": "Original Developer"
        }
    ],
    "require": {
        "php": ">=7.1.0",
        "dragonmantank/cron-expression": "^2.2",
        "illuminate/console": "^5.6|^5.7"
    },
    "require-dev": {
        "illuminate/cache": "^5.6",
        "orchestra/testbench": "^3.7",
        "phpunit/phpunit": "^7.3"
    },
    "autoload": {
        "psr-4": {
            "EagleDevelopers\\NovaScheduledTasks\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "EagleDevelopers\\NovaScheduledTasks\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "EagleDevelopers\\NovaScheduledTasks\\ToolServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
